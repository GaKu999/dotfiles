#!/bin/sh
# vim:ft=sh:noet:ts=3:sw=3:
# file:$XDG_BIN_DIR/.overrides/wmenu
#==============================================================================#


exec /usr/bin/wmenu -bi -f"monospace 12" \
	-N151515f2 -nd0d0d0 \
	-M151515f2 -md0d0d0 \
	-S505050 -sf5f5f5 "$@"


#==============================================================================#
#                                 END OF FILE                                  #
#==============================================================================#
