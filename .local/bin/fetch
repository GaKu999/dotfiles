#!/bin/sh
# vim:ft=sh:fenc=utf-8:noet:ts=3:sw=3:
# file:$XDG_BIN_DIR/fetch
#==============================================================================#


url="${1:-$(wl-paste)}"
[ -n "$url" ] && curl --silent -- "$url" | xdg-open


